
# THANOS System Environment Variables

# Abacus AI Configuration
ABACUSAI_API_KEY="your-abacus-ai-api-key-here"

# SnapOrchestrator Webhook URL (Required for production orchestration)
SNAP_ORCHESTRATOR_WEBHOOK_URL="https://api.abacus.ai/v1/webhooks/your-webhook-id"

# Database Configuration
DATABASE_URL="file:./prisma/dev.db"

# NextAuth Configuration
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-here"

# Optional: Additional Abacus AI Configuration
ABACUS_PROJECT_ID="your-project-id"
ABACUS_ORGANIZATION_ID="your-org-id"

# Optional: Advanced Features
ENABLE_FACE_RECOGNITION="false"
ENABLE_OCR_PROCESSING="false"
ENABLE_VOICE_PROCESSING="false"

# Optional: Performance Configuration
MAX_FILES_PER_ORGANIZATION="1000"
ORGANIZATION_TIMEOUT_SECONDS="1800"
CONCURRENT_PROCESSING_LIMIT="10"
